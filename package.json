{
  "name": "stratus-site-skeleton",
  "version": "1.0.0",
  "description": "Stratus Site Skeleton for RV",
  "hexo": {
    "version": "3.7.1"
  },
  "scripts": {
    "dev": "export NODE_ENV=development && gulp --gulpfile ./node_modules/stratus-build-tools/gulpfile.js",
    "dev-build": "export NODE_ENV=development && gulp build --gulpfile ./node_modules/stratus-build-tools/gulpfile.js",
    "production": "export NODE_ENV=production && gulp build --gulpfile ./node_modules/stratus-build-tools/gulpfile.js",
    "test": "mocha",
    "preinstall": "if ! node --version | grep 8; then echo \"Node version is not compatible with stratus, please use node v8.9.x\" && exit 1; fi"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "foundation-sites": "6.4.4-rc1",
    "gulp": "^3.9.1",
    "hexo-browsersync": "0.3.0",
    "hexo-generator-archive": "0.1.5",
    "hexo-generator-category": "0.1.3",
    "hexo-generator-index": "0.2.1",
    "hexo-generator-tag": "0.2.0",
    "hexo-renderer-ejs": "0.3.1",
    "hexo-renderer-marked": "0.3.0",
    "hexo-renderer-stylus": "0.3.3",
    "hexo-server": "0.2.2",
    "stratus-build-tools": "git+ssh://git@stash.redventures.net/stratuscore/stratus-build-tools.git"
  }
}